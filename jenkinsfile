pipeline {
    agent any
    tools {
    maven 'mvn'
}


    stages {
    
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/shivaprasadn011-hub/dec09_java.git'
            }
        }
        stage('version') {
            steps {
                sh 'mvn --version'
            }
        }
      
        stage('Deploy') {
            steps {
                echo "Deploying..."
            }
        }
    }
}
