pipeline {
    agent any
    tools {
    maven 'mvn'
}


    stages {
    
        stage('samana_Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/shivaprasadn011-hub/dec09_java.git'
            }
        }
        stage('samana_version') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }

        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }

        stage('Deploy') {
            steps {
                echo "Deploying..."
            }
        }
    }
}
